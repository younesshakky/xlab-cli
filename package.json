{
  "name": "xlab-cli",
  "version": "1.0.0-alpha",
  "author": "younesshakky @younesshakky",
  "bin": {
    "xlab-cli": "./bin/xlab-cli"
  },
  "bugs": "https://github.com/younesshakky/xlab-cli/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-autocomplete": "^0.1.0",
    "@oclif/plugin-help": "^2",
    "@types/fs-extra": "^5.0.4",
    "axios": "^0.18.0",
    "joi": "^14.3.1",
    "pkg": "^4.3.5",
    "query-string": "^6.2.0",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@oclif/tslint": "^3",
    "@types/chai": "^4",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "chai": "^4",
    "globby": "^8",
    "mocha": "^5",
    "nyc": "^13",
    "rimraf": "^2.6.2",
    "ts-node": "^7",
    "tslint": "^5",
    "typescript": "^3.0"
  },
  "bundledDependencies": [
    "axios",
    "cli-ux",
    "@oclif/command",
    "@oclif/config",
    "@oclif/plugin-help",
    "@oclif/errors",
    "query-string",
    "joi"
  ],
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/younesshakky/xlab-cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "xlab-cli",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-autocomplete"
    ]
  },
  "repository": "younesshakky/xlab-cli",
  "scripts": {
    "postpack": "rimraf -rf oclif.manifest.json",
    "posttest": "tslint -p test -t stylish",
    "prepack": "rimraf -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "pack": "./node_modules/.bin/oclif-dev pack",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md",
    "start": "./bin/xlab-cli"
  },
  "types": "lib/index.d.ts",
  "pkg.scripts": "./lib/**/*.js"
}
